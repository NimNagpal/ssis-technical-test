          SQLTask:Connection="{67FE4213-2F90-4E5E-9037-1935BA370C1C}"
          SQLTask:SqlStatementSource="&#xA;Declare @Dates TABLE(DateValue DATE)&#xA;DECLARE @StartDate date&#xA;DECLARE @EndDate date&#xA; Select @StartDate =  Min(orderDate), @EndDate = Max(orderDate) from FactSalesOrder&#xA;&#xA;DECLARE @CUR_DATE DATE&#xA;    SET @CUR_DATE = @StartDate&#xA;    WHILE @CUR_DATE &lt;= @EndDate BEGIN&#xA;        INSERT INTO @Dates VALUES(convert(varchar(8),@CUR_DATE,112))&#xA;        SET @CUR_DATE = DATEADD(DAY, 1, @CUR_DATE)&#xA;&#x9;&#x9;END&#xA;&#xA;INSERT INTO  FactCurrencyRate &#xA;SELECT dc.CurrencyKey, convert(varchar(8),NewCurr.DateValue,112) datekey, NewCurr.fxRate As AverageRate, NewCurr.fxRate As EndOfDayRate, CAST(NewCurr.DateValue as datetime) As date&#xA;FROM &#xA;&#x9;(&#x9;SELECT   d.DateValue, MIN(fncr.FromCurrency) FromCurrency, MIN(fncr.Tocurrency) Tocurrency, MIN(fncr.fxRate) fxRate&#xA;&#x9;&#x9;FROM    @Dates d&#xA;&#x9;&#x9;&#x9;&#x9;OUTER APPLY (SELECT TOP 1 * FROM AdventureWorks2016.Sales.NewCurencyRate f  WHERE f.date &lt;= d.DateValue ORDER BY f.date DESC) fncr&#xA;&#x9;&#x9;WHERE   d.DateValue &lt;= (SELECT MAX(date) FROM AdventureWorks2016.Sales.NewCurencyRate)&#xA;&#x9;&#x9;GROUP   BY d.DateValue&#x9;) NewCurr&#xA;&#x9;INNER JOIN DimCurrency dc on dc.CurrencyAlternateKey = NewCurr.Tocurrency&#xA;&#x9;LEFT JOIN FactCurrencyRate fcr &#xA;&#x9;ON fcr.CurrencyKey = dc.CurrencyKey and convert(varchar(8),NewCurr.DateValue,112) = fcr.DateKey&#xA;&#x9;WHERE fcr.CurrencyKey IS NULL&#xA;&#x9;AND dc.CurrencyKey IS NOT NULL" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
